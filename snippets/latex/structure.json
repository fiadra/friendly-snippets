{
    "Section": {
        "prefix": ["section", "\\section"],
        "body": ["\\section{$1}$0"],
        "description": "Insert a new section."
    },
    "Subsection": {
        "prefix": ["subsection", "\\subsection"],
        "body": ["\\subsection{$1}$0"],
        "description": "Insert a new subsection."
    },
    "Chapter": {
        "prefix": "cha",
        "body": [
            "\\chapter{$1}\\label{chap:${1/([a-zA-Z]+)|([^a-zA-Z]+)/${1:/downcase}${2:+_}/g}} % (fold)",
            "${0:$TM_SELECTED_TEXT}",
            "% chapter $1 (end)"
        ],
        "description": "Chapter"
    },
    "Paragraph": {
        "prefix": "par",
        "body": [
            "\\paragraph{${1:paragraph name}}\\label{par:${1/([a-zA-Z]+)|([^a-zA-Z]+)/${1:/downcase}${2:+_}/g}} % (fold)",
            "${0:$TM_SELECTED_TEXT}",
            "% paragraph $1 (end)"
        ],
        "description": "Paragraph"
    },
    "Page": {
        "prefix": "page",
        "body": ["${1:page}~\\pageref{$2}$0"],
        "description": "Page"
    }
}
